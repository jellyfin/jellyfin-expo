{
  "expo": {
    "newArchEnabled": true,
    "name": "Jellyfin",
    "slug": "jellyfin-expo",
    "owner": "jellyfin",
    "platforms": [
      "ios"
    ],
    "version": "1.7.0",
    "orientation": "default",
    "primaryColor": "#00a4dc",
    "backgroundColor": "#101010",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#101010"
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "5.6.0.0",
      "supportsTablet": true,
      "requireFullScreen": false,
      "bundleIdentifier": "org.jellyfin.expo",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera.",
        "UIBackgroundModes": [
          "audio",
          "fetch"
        ]
      }
    },
    "description": "Jellyfin Mobile",
    "plugins": [
      "expo-asset",
      "expo-font",
      "expo-localization"
    ]
  }
}
