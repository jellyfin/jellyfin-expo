{
  "name": "jellyfin-ios",
  "version": "1.7.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo prebuild --platform ios",
    "lint": "eslint \".\"",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "moduleNameMapper": {
      "@jellyfin/ux-ios/.+\\.png$": "<rootDir>/__mocks__/assetFileMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|normalize-url|axios|@jellyfin/sdk(/.*)?)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/metro.config.js",
      "!**/jest.setup.js"
    ],
    "testEnvironment": "jsdom"
  },
  "browserslist": [
    "iOS > 11",
    "Android >= 5"
  ],
  "dependencies": {
    "@jellyfin/sdk": "0.11.0",
    "@jellyfin/ux-ios": "1.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "@react-navigation/stack": "6.4.1",
    "axios": "1.9.0",
    "compare-versions": "3.6.0",
    "expo": "~46.0.21",
    "expo-application": "~4.2.2",
    "expo-asset": "~8.6.3",
    "expo-av": "~12.0.4",
    "expo-constants": "~13.2.4",
    "expo-device": "~4.3.0",
    "expo-file-system": "~14.1.0",
    "expo-font": "~10.2.0",
    "expo-keep-awake": "~10.2.0",
    "expo-localization": "~13.1.0",
    "expo-screen-orientation": "~4.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-web-browser": "~11.0.0",
    "i18next": "21.10.0",
    "normalize-url": "7.2.0",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-i18next": "11.18.6",
    "react-lifecycles-compat": "3.0.4",
    "react-native": "0.69.9",
    "react-native-elements": "3.4.3",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-url-polyfill": "1.3.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.0",
    "uuid": "11.1.0",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "7.26.8",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/jest-native": "4.0.13",
    "@testing-library/react": "16.1.0",
    "@testing-library/react-native": "7.2.0",
    "@types/jest": "27.5.2",
    "@types/react": "~18.0.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-preset-expo": "~9.2.0",
    "domexception": "4.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-native": "3.11.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "26.6.2",
    "jest-expo": "^46.0.0",
    "jest-fetch-mock": "3.0.3",
    "node-abort-controller": "3.1.1",
    "ts-jest": "26.5.6",
    "typescript": "^4.6.3"
  },
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
