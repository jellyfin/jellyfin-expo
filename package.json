{
  "name": "jellyfin-expo",
  "version": "1.7.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build": "expo prebuild --platform ios",
    "lint": "eslint \".\"",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|normalize-url|axios|@jellyfin/sdk(/.*)?)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/metro.config.js",
      "!**/jest.setup.js"
    ]
  },
  "browserslist": [
    "iOS >= 15.1",
    "Android >= 5"
  ],
  "dependencies": {
    "@jellyfin/sdk": "0.7.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "7",
    "@react-navigation/native": "~7",
    "@react-navigation/native-stack": "~7",
    "@react-navigation/stack": "~7",
    "compare-versions": "3.6.0",
    "expo": "^52.0.0",
    "expo-asset": "~11.0.1",
    "expo-av": "~15.0.1",
    "expo-constants": "~17.0.3",
    "expo-device": "~7.0.1",
    "expo-file-system": "~18.0.5",
    "expo-font": "~13.0.1",
    "expo-keep-awake": "~14.0.1",
    "expo-localization": "~16.0.0",
    "expo-screen-orientation": "~8.0.1",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "i18next": "21.10.0",
    "normalize-url": "7.2.0",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "11.18.6",
    "react-lifecycles-compat": "3.0.4",
    "react-native": "0.76.5",
    "react-native-elements": "3.4.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-url-polyfill": "1.3.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "uuid": "8.3.2",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "7.25.9",
    "@react-native-community/cli": "15.1.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/jest-native": "4.0.13",
    "@testing-library/react": "16.1.0",
    "@testing-library/react-native": "12.9.0",
    "@types/jest": "27.5.2",
    "@types/react": "18.3.16",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-preset-expo": "~12.0.0",
    "domexception": "4.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-compat": "3.13.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-native": "3.11.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "jest-expo": "~52.0.2",
    "jest-fetch-mock": "3.0.3",
    "node-abort-controller": "3.1.1",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.3.3"
  },
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  }
}
